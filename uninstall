#!/bin/bash
: 'Title           : uninstall
   Description     : This file is generated by running install file'

#delete the directory the app is installed
rm -r -f "/home/keisuke/.mysqlinsert"

#remove export from .bashrc or .profile
sed -e 's/export PATH=\"\/home\/keisuke\/.mysqlinsert\/mysqlinsert:$PATH\"//g' -i "/home/keisuke/.bashrc"

echo "uninstalled successfully"
